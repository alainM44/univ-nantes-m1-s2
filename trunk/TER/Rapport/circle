/* Problem: consider a circle given by a center (x0,y0) and a radius R.
            Given a real D>=0, find all the points on the circle which
            distance to the origin is D.
*/

Branch number=1;

Constants
  x0 = 2,
  y0 = 2,
  R0 = 2,
  x1 = 5,
  y1 = 2,
  R1 = 2
;

Variables
  x in ]-oo, +oo[,
  y in ]-oo, +oo[
;

Constraints
  (x-x0)^2 + (y-y0)^2 = R0^2,
  (x-x1)^2 + (y-y1)^2 = R1^2
;
